{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:prettier/recommended",
        "standard",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier":[
            "error",
            {
                "endOfLine":"auto"
            }
        ],
        "space-before-function-paren": "off",
        "comma-dangle":"off",
        "multiline-ternary":"off",
        "react/prop-types": "off"
    }
}
